show databases;
create database movielens;

show tables;


DROP TABLE IF EXISTS movies;

CREATE EXTERNAL TABLE IF NOT EXISTS movies (
MovieID INT,
Title STRING,
Genres STRING)
ROW FORMAT DELIMITED FIELDS TERMINATED BY '@';

